{
  "name": "momocraft-backend",
  "version": "0.1.0",
  "description": "a microservice for user authenticate",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo",
    "build": "nest build",
    "start": "node dist/main.js",
    "dev": "yarn prebuild && DEBUG=momocraft:*,typeorm:* NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "DEBUG=momocraft:*,typeorm:* jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typecheck": "tsc --noEmit --incremental false",
    "typecheck:watch": "tsc --watch --noEmit --incremental false",
    "typeorm": "ts-node node_modules/typeorm/cli.js --config typeorm-dev.env",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "dependencies": {
    "@jokester/ts-commonutil": "0.2.2",
    "@nestjs/common": "^7.3.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/platform-express": "^7.3.1",
    "@nestjs/swagger": "^4.5.12",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "debug": "^4.1.1",
    "fp-ts": "^2",
    "gravatar-url": "^3.1.0",
    "nanoid": "^3.1.10",
    "openid-client": "^3.15.6",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "swagger-ui-express": "^4.1.4",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.3.1",
    "@openapitools/openapi-generator-cli": "^1.0.15-4.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26",
    "@types/node": "<13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "<27",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "<27",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "<4"
  },
  "resolutions": {
    "@types/node": "12"
  }
}
